{
  "name": "Koulu",
  "version": "1.0",
  "description": "Cookie tracker",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "config.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon32.png"
  },
  "permissions": [
    "alarms",
    "cookies",
    "storage",
    "contextMenus",
    "*://*.somedomain.com/",
    "*://localhost:*/"
  ]
}